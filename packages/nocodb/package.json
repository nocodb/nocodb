  "scripts": {
    "build": "pnpm run docker:build",
    "docker:build": "EE=\"true-xc-test\" rspack  --config docker/rspack.config.js",
    "build:obfuscate": "EE=true rspack --config rspack.config.js",
    "build:cli:module": "rspack --config rspack.cli.config.js",
    "build:on-prem": "EE=true rspack --config rspack.ee-on-prem.config.js",
    "build:cloud": "EE=true rspack --config rspack.ee-cloud.config.js",
    "obfuscate:build:publish": "pnpm run build:obfuscate && pnpm publish .",
    "registerIntegrations": "node build-utils/registerIntegrations.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "pnpm run watch:run",
    "start:prod": "node docker/main",
    "test:unit": "cross-env NODE_ENV=test TS_NODE_PROJECT=tests/unit/tsconfig.json mocha --require @swc-node/register tests/unit/index.test.ts --recursive --timeout 300000  --exit --delay",
    "test:unit:pg:ee": "cp tests/unit/.pg.env tests/unit/.env; cross-env EE=true NODE_ENV=test TS_NODE_PROJECT=tests/unit/tsconfig.ee.json NODE_ENV=test NC_DISABLE_PG_DATA_REFLECTION=true mocha --require @swc-node/register tests/unit/index.test.ts --recursive --timeout 300000  --exit --delay",
    "test:unit:pg": "cp tests/unit/.pg.env tests/unit/.env; cross-env TS_NODE_PROJECT=tests/unit/tsconfig.json NODE_ENV=test mocha -r @swc-node/register tests/unit/index.test.ts --recursive --timeout 300000  --exit --delay",
    "watch:run": "cross-env NODE_ENV=development NC_DISABLE_TELE=true EE=true ENTRYPOINT=src/run/docker rspack --config rspack.dev.js",
    "watch:run:ee": "cross-env NODE_ENV=development NC_DISABLE_TELE=true EE=true ENTRYPOINT=src/ee/run/docker rspack --config rspack.dev.ee.js",
    "watch:run:ee-on-prem": "cross-env NODE_ENV=development NC_DISABLE_TELE=true EE=true ENTRYPOINT=src/ee/run/docker rspack --config rspack.dev.ee-on-prem.js",
    "watch:run:ee-cloud": "cross-env NODE_ENV=development NC_DISABLE_TELE=true EE=true ENTRYPOINT=src/ee/run/docker rspack --config rspack.dev.ee-cloud.js",
    "watch:run:mysql": "cross-env NODE_ENV=development NC_DISABLE_TELE=true ENTRYPOINT=src/run/dockerRunMysql rspack --config rspack.dev.js",
    "watch:run:mysql:ee": "cross-env NODE_ENV=development NC_DISABLE_TELE=true EE=true ENTRYPOINT=src/ee/run/dockerRunMysql rspack --config rspack.dev.ee.js",
    "watch:run:pg": "cross-env NODE_ENV=development NC_DISABLE_TELE=true ENTRYPOINT=src/run/dockerRunPG rspack --config rspack.dev.js",
    "watch:run:pg:ee": "cross-env NODE_ENV=development NC_DISABLE_TELE=true EE=true ENTRYPOINT=src/ee/run/dockerRunPG rspack --config rspack.dev.ee.js",
    "watch:run:playwright:mysql": "rm -f ./test_noco.db; cross-env NODE_ENV=test NC_DB=\"mysql2://localhost:3306?u=root&p=password&d=pw_ncdb\" PLAYWRIGHT_TEST=true NC_DISABLE_TELE=true EE=true ENTRYPOINT=src/run/testDocker rspack --config rspack.dev.ee.js",
    "watch:run:playwright": "rm -f ./test_noco.db; cross-env NODE_ENV=test DATABASE_URL=sqlite:./test_noco.db PLAYWRIGHT_TEST=true NC_DISABLE_TELE=true EE=true NC_SNAPSHOT_WINDOW_SEC=3 ENTRYPOINT=src/run/testDocker rspack --config rspack.dev.ee.js",
    "watch:run:playwright:pg": "rm -f ./test_noco.db; cross-env NODE_ENV=test NC_DB=\"pg://localhost:5432?u=postgres&p=password&d=pw_ncdb\" PLAYWRIGHT_TEST=true NC_DISABLE_TELE=true EE=true ENTRYPOINT=src/run/testDocker rspack --config rspack.dev.ee.js",
    "watch:run:playwright:pg:ee": "rm -f ./test_noco.db; cross-env NODE_ENV=test NC_DB=\"pg://localhost:5432?u=postgres&p=password&d=pw_ncdb_meta\" NC_DATA_DB=\"pg://localhost:5432?u=postgres&p=password&d=pw_ncdb_data\" PLAYWRIGHT_TEST=true NC_DISABLE_TELE=true EE=true ENTRYPOINT=src/ee/run/testDocker rspack --config rspack.dev.ee.js",
    "watch:run:playwright:quick": "rm -f ./test_noco.db; cp ../../tests/playwright/fixtures/noco_0_91_7.db ./test_noco.db; cross-env DATABASE_URL=sqlite:./test_noco.db NC_DISABLE_TELE=true NODE_ENV=test EE=true ENTRYPOINT=src/run/docker rspack --config rspack.dev.ee.js",
    "watch:run:playwright:pg:cyquick": "rm -f ./test_noco.db; cp ../../tests/playwright/fixtures/noco_0_91_7.db ./test_noco.db; cross-env NC_DISABLE_TELE=true NODE_ENV=test EE=true ENTRYPOINT=src/run/dockerRunPG_CyQuick.ts rspack --config rspack.dev.ee.js",
    "mail": "email dev --dir src/services/mail/templates --port 8001",
    "mail:ee": "email dev --dir src/ee/services/mail/templates --port 8002",
    "test": "jest --runInBand --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r @swc-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
